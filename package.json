{
  "name": "@lsby/net-core",
  "version": "0.0.16",
  "type": "module",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js"
  },
  "bin": {
    "lsby-net-core-gen-list": "./dist/bin/gen-list/bin.cjs",
    "lsby-net-core-gen-test": "./dist/bin/gen-test/bin.cjs",
    "lsby-net-core-gen-type": "./dist/bin/gen-type/bin.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:all": "npm run build:cjs && npm run build:esm && npm run build:bin",
    "build:bin": "tsup src/bin/**/*.ts --external typescript --format cjs --clean --dts -d dist/bin",
    "build:cjs": "tsup src/**/*.ts --external typescript --format cjs --clean --dts -d dist/cjs",
    "build:esm": "tsup src/**/*.ts --external typescript --format esm --clean --dts -d dist/esm",
    "other:clean": "rm -rf dist",
    "other:format": "prettier --write .",
    "other:lint": "eslint . --fix",
    "other:pub": "npm run other:typecheck && npm run other:lint && npm run build:all && bumpp && pnpm -r publish --access public",
    "other:typecheck": "tsc --noEmit",
    "other:typecheck:watch": "tsc --noEmit -w",
    "run:start": "tsx src/index.ts"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "debug": "^4.3.5",
    "express": "^4.19.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.12.10",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "bumpp": "^9.4.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "prettier": "3.2.5",
    "prettier-plugin-packagejson": "^2.5.0",
    "tsup": "^8.0.2",
    "tsx": "^4.9.3",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@types/express": "^4.17.21"
  }
}
