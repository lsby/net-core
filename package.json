{
  "name": "@lsby/net-core-monorepo",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lsby/net-core.git"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm -r --filter=@lsby/net-core run _build:all",
    "check:all": "pnpm -r run _check:all",
