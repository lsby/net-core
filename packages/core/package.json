{
  "name": "@lsby/net-core",
  "version": "0.13.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsby/net-core.git"
  },
  "type": "module",
  "exports": {
    "require": "./dist/cjs/index.cjs",
    "import": "./dist/esm/index.js"
  },
  "bin": {
    "lsby-net-core-gen-api-list": "./dist/bin/gen-api-list/bin.cjs",
    "lsby-net-core-gen-api-type": "./dist/bin/gen-api-type/bin.cjs",
    "lsby-net-core-gen-test": "./dist/bin/gen-test/bin.cjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "_build:all": "npm run _build:cjs && npm run _build:esm && npm run _build:bin",
    "_build:bin": "tsup src/bin/**/*.ts --external typescript --format cjs --clean --dts -d dist/bin",
    "_build:cjs": "tsup src/**/*.ts --external typescript --format cjs --clean --dts -d dist/cjs",
    "_build:esm": "tsup src/**/*.ts --external typescript --format esm --clean --dts -d dist/esm",
    "_check:all": "npm run _check:format && npm run _check:lint",
    "_check:format": "prettier --write .",
    "_check:lint": "eslint . --fix",
    "publish": "npm run _build:all && bumpp && pnpm publish"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "short-uuid": "^5.2.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@lsby/eslint-config": "^0.3.15",
    "@lsby/eslint-plugin": "^0.0.30",
    "@types/node": "^20.12.10",
    "@types/ws": "^8.5.13",
    "bumpp": "^9.4.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-sort-class-members": "^1.20.0",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^2.5.20",
    "tsup": "^8.5.0",
    "typescript": "<5.6.0"
  },
  "peerDependencies": {
    "@lsby/ts-fp-data": "^0.2.5",
    "@lsby/ts-log": "^0.3.0",
    "@types/express": "^4.17.21",
    "express": "^4.19.2",
    "zod": "3.24.2"
  },
  "packageManager": "pnpm@9.7.1"
}
